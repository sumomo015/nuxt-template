    "@vueuse/core": "^11.0.1",
    "@vueuse/nuxt": "^11.0.1",
    "eslint": "^9.9.1",
    "happy-dom": "^15.0.0",
    "npm-check-updates": "^17.1.0",
    "nuxt-security": "^1.4.3",
    "prettier": "^3.3.3",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.2.0",
    "vite": "^5.4.2",
    "vitest": "^2.0.5",
    "vue": "^3.4.38",
    "vue-tsc": "^2.0.29"
